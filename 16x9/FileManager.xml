<?xml version="1.0" encoding="UTF-8"?>
<window>
    <!-- filemanager -->
    <backgroundcolor>Background</backgroundcolor>
    <defaultcontrol always="true">20</defaultcontrol>

    <controls>

        <!-- Background -->
        <include>Furniture_Background</include>
        <include>Furniture_TopBar</include>
        <include>Furniture_RSS</include>

        <!-- Main group -->
        <control type="group">
            <left>sidepad</left>
            <right>sidepad</right>
            <!-- Left group -->
            <control type="group">
                <left>bigpad</left>
                <width>settingshalfw</width>
                <top>top</top>
                <include>Animation_Hub</include>

                <include content="Dialog_FileManager_List">
                    <param name="width" value="settingshalfw" />
                    <param name="labelwidth" value="761" />
                    <param name="id" value="20" />
                    <param name="onright" value="21" />
                    <param name="pathid" value="101" />
                </include>

            </control>

            <!-- Right group -->
            <control type="group">
                <right>bigpad</right>
                <width>settingshalfw</width>
                <top>top</top>
                <include>Animation_Common</include>

                <include content="Dialog_FileManager_List">
                    <param name="width" value="settingshalfw" />
                    <param name="labelwidth" value="761" />
                    <param name="id" value="21" />
                    <param name="onleft" value="20" />
                    <param name="pathid" value="102" />
                </include>
            </control>

        </control>

    </controls>

</window>